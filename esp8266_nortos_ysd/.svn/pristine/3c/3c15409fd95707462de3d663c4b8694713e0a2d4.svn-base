static void ICACHE_FLASH_ATTR dn_resolve_dis_domain_proc(dn_t *dn, dn_event_t *event)
{
	u_int32_t test_ip = 0;
	
	if (event->type != DN_EVENT_TIME)
		return;

	dn_flush_dns_cache();
	
	INFO("judge doname is OK!\r\n");

	/* 没有成功就一直解析 */
	if (! dn_resovle_ok(dn)){
