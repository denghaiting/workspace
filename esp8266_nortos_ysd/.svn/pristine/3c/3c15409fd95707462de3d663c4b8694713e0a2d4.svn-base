	}

	/*域名的IP不是真正域名服务器返回的，而是路由器网关拦截返回的。*/
	test_ip = dn_find_valid_ip(dn);
	if (! dn_real_dns_ip(test_ip, dn)){
		DN_ERROR("dn_real_dns_ip ERROR\r\n");
